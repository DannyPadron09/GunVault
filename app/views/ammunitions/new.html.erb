<%= form_for :ammunition do |f| %>
    <div class="form-group">
        <%= f.label :caliber %>
        <%= f.text_field :caliber, class: "form-control" %>
    </div>

    <div class="form-group">
        <%= f.label :quantity %>
        <%= f.text_field :quantity, class: "form-control" %>
    </div>

    <div class="form-group">
        <%= f.hidden_field :owner_id, value: @owner.id %><br>
    </div>

    <div class="actions">
        <%= f.submit "Add New Ammo" %>
    </div>
<% end %>

